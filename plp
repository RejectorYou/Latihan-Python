import random,time,os

j=""
while len(j)<=100:
    j+="="
i=100
while(i>=0):
    k=random.randrange(1,100)
    print("Loading ",i,"%")
    print(j)
    j=j[:-1]
    i-=1
    k=k%3
    if(k<1):
        time.sleep(0.01)
    elif(k<2):
        time.sleep(0.001)
    else:
        time.sleep(0.01)
    os.system('cls')